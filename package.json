{
  "name": "conjugate-esp",
  "version": "0.4.0",
  "description": "Spanish verb conjugator",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "prepare": "npm run build",
    "test": "tsc && jest -b1 --noStackTrace ",
    "verbose": "tsc && jest -b1 --verbose",
    "strings": "tsc && jest -b1 --noStackTrace --testPathPattern=stringutils",
    "cover": "jest --coverage",
    "lint": "eslint --ext .ts",
    "flint": "eslint --fix --ext .ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jirimracek/conjugate-esp.git"
  },
  "keywords": [
    "spanish",
    "verb",
    "conjugation",
    "nodejs",
    "JavaScript",
    "TypeScript"
  ],
  "author": "Jiri Mracek",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jirimracek/conjugate-esp/issues"
  },
  "homepage": "https://github.com/jirimracek/conjugate-esp#readme",
  "dependencies": {},
  "devDependencies": {
    "@types/jest": "^25.2.1",
    "@types/node": "^13.13.2",
    "@typescript-eslint/eslint-plugin": "^2.29.0",
    "@typescript-eslint/parser": "^2.29.0",
    "coveralls": "^3.0.13",
    "eslint": "^6.8.0",
    "eslint-config-typescript": "^3.0.0",
    "jest": "^25.4.0",
    "ts-jest": "^25.4.0",
    "typescript": ">=3.8.3"
  }
}
